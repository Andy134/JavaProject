		// TODO Auto-generated method stub
		// 1. Kieu du lieu + ten bieb + gia tri khoi tao(ko bat buoc)
		// ten bien chua cac ky tu Unicoxde, chu so, ky tu
		// phan biet Hoa thuong
		// khuyen khich la tieng Anh
		// tranh: khong trung tu khoa trong java, khong bat dau bang so, ko chua
		// ky tu dac biet

		int age = 23;
		
		//xuat thong bao
		System.out.println("tuổi" + age);
		
		// 2.Kieu du lieu trong java
		// kiểu dữ liệu nguyên thủy ( primitive )
		// ------------- tham chiếu int, long , byte, short, char - Kieu Nguyen ASCI
		char c = 64;
		int x = 6;
		long y = 4;
		byte z = 5;
		short ages = 27;
		
		System.out.println(c);
		
		//double, float
		
		
		// boolean
		
		
